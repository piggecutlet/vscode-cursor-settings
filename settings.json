{
  // プライバシー
  "crashReporting.enabled": "off", // Cursor
  "dotnetAcquisitionExtension.enableTelemetry": false,
  "telemetry.feedback.enabled": false,
  "telemetry.telemetryLevel": "off", // VSCode

  // 自動更新
  // 本体
  "update.enableWindowsBackgroundUpdates": false,
  // 勝手に "update.mode": "none", が消えることがある
  "update.mode": "none",
  "update.showReleaseNotes": false,
  // 拡張機能
  "extensions.autoCheckUpdates": false,
  "extensions.autoUpdate": false,

  // フォーマット
  "editor.codeActionsOnSave": {
    "source.addMissingImports": "explicit",
    "source.organizeImports": "explicit",
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "[csharp]": {
    // Cursor版 anysphere.csharp
    "editor.defaultFormatter": "ms-dotnettools.csharp",
    "editor.formatOnType": false,
  },

  "eslint.enable": true,

  // VSCode
  "chat.disableAIFeatures": true,

  // Cursor
  // Cursor Tabをmarkdownとplaintextでも有効にする
  "cursor.cpp.disabledLanguages": [],
  // 使用量を表示
  "cursor.composer.usageSummaryDisplay": "always",

  // Git
  // GitLensの代わり
  "git.blame.editorDecoration.enabled": true,
  // .gitディレクトリを表示
  "files.exclude": {
    "**/.git": false,
  },

  // Hex Editor
  "workbench.editorAssociations": {
    "*.bytes": "hexEditor.hexedit",
    "*.dat": "hexEditor.hexedit",
    "*.so": "hexEditor.hexedit",
  },

  // VSCodeに寄せる
  "workbench.sideBar.location": "left",

  // その他
  // package.json デバッグ 非表示
  "debug.javascript.codelens.npmScripts": "never",
  // C# x個の参照 を非表示
  "editor.codeLens": false,
  // コメントの最初に追加されるタブの数
  "editor.tabSize": 2,
  // ファイルのtabSizeを優先するか
  "editor.detectIndentation": false,
  // ASCII文字以外を強調表示
  "editor.unicodeHighlight.nonBasicASCII": false,
  // ハイフンで単語を区切らない
  "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",

  // 制限モード
  "security.workspace.trust.enabled": false,
  // アクティビティバー 垂直
  "workbench.activityBar.orientation": "vertical", // Cursor
  "workbench.startupEditor": "none",
  // コマンドパレット
  "window.commandCenter": true,
  "extensions.ignoreRecommendations": true,
  "git.openRepositoryInParentFolders": "never",

  // 改行で行を変更
  "markdown.preview.breaks": true,

  // 空白行
  // 末尾に改行
  "files.insertFinalNewline": true,
  // 有効にすると、ファイルの保存時に最終行以降の新しい行をトリミングします。
  "files.trimFinalNewlines": true,
  // 有効にすると、ファイルの保存時に末尾の空白をトリミングします。
  "files.trimTrailingWhitespace": true,

  "diffEditor.ignoreTrimWhitespace": false,

  // 縦線の色を階層ごとに変える
  "editor.guides.bracketPairs": true,

  // フォント
  // 等幅フォント UDEVGothicJPDOC-Regular.ttf を追加
  "editor.fontFamily": "'UDEV Gothic JPDOC', Consolas, 'Courier New', monospace",

  // 斜体をなくす
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "comment.block",
          "comment.block.documentation",
          "comment.line",
          "constant",
          "constant.character",
          "constant.character.escape",
          "constant.numeric",
          "constant.numeric.integer",
          "constant.numeric.float",
          "constant.numeric.hex",
          "constant.numeric.octal",
          "constant.other",
          "constant.regexp",
          "constant.rgb-value",
          "emphasis",
          "entity",
          "entity.name",
          "entity.name.class",
          "entity.name.function",
          "entity.name.method",
          "entity.name.section",
          "entity.name.selector",
          "entity.name.tag",
          "entity.name.type",
          "entity.other",
          "entity.other.attribute-name",
          "entity.other.inherited-class",
          "invalid",
          "invalid.deprecated",
          "invalid.illegal",
          "keyword",
          "keyword.control",
          "keyword.operator",
          "keyword.operator.new",
          "keyword.operator.assignment",
          "keyword.operator.arithmetic",
          "keyword.operator.logical",
          "keyword.other",
          "markup",
          "markup.bold",
          "markup.changed",
          "markup.deleted",
          "markup.heading",
          "markup.inline.raw",
          "markup.inserted",
          "markup.italic",
          "markup.list",
          "markup.list.numbered",
          "markup.list.unnumbered",
          "markup.other",
          "markup.quote",
          "markup.raw",
          "markup.underline",
          "markup.underline.link",
          "meta",
          "meta.block",
          "meta.cast",
          "meta.class",
          "meta.function",
          "meta.function-call",
          "meta.preprocessor",
          "meta.return-type",
          "meta.selector",
          "meta.tag",
          "meta.type.annotation",
          "meta.type",
          "punctuation.definition.string.begin",
          "punctuation.definition.string.end",
          "punctuation.separator",
          "punctuation.separator.continuation",
          "punctuation.terminator",
          "storage",
          "storage.modifier",
          "storage.type",
          "string",
          "string.interpolated",
          "string.other",
          "string.quoted",
          "string.quoted.double",
          "string.quoted.other",
          "string.quoted.single",
          "string.quoted.triple",
          "string.regexp",
          "string.unquoted",
          "strong",
          "support",
          "support.class",
          "support.constant",
          "support.function",
          "support.other",
          "support.type",
          "support.type.property-name",
          "support.variable",
          "variable",
          "variable.language",
          "variable.name",
          "variable.other",
          "variable.other.readwrite",
          "variable.parameter",
          "comment.line.double-slash.ts",
          "meta.block.ts",
          "meta.function.ts",
          "source.ts",
          "comment.line.double-slash.js",
          "meta.structure.array.json.comments",
          "meta.structure.dictionary.value.json.comments",
          "meta.structure.dictionary.json.comments",
          "meta.structure.array.json.comments",
          "meta.structure.dictionary.value.json.comments",
          "meta.structure.dictionary.json.comments",
          "meta.structure.dictionary.value.json.comments",
        ],
        "settings": {
          "fontStyle": "",
        },
      },
    ],
  },

  // 新規
}
